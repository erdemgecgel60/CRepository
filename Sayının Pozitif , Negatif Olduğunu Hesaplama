#include <stdio.h>
#include <stdbool.h>
#include <math.h>
int main()
{


    long long int sayi;

    printf("Sayi : ");

    scanf("%lld",&sayi);

    if (sayi > 0)
    {

        printf("Positive");


    }

    else if (sayi == 0)
    {

        printf("Notr");

    }

    else 
    {

        printf("Negative!");

    }

 
}
